{
  "name": "tyjow/active",
  "description": "The helper class for Laravel applications to get active class base on current route",
  "keywords": [
    "laravel",
    "active",
    "routing"
  ],
  "license": "MIT",
  "homepage": "https://www.hieule.info/tag/laravel-active/",
  "authors": [
    {
      "name": "Tyjow",
      "email": "platon.jonathan@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "orchestra/testbench": "^6.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Tyjow\\Active\\": "src/",
      "Tyjow\\ActiveTest\\": "tests/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "Tyjow\\Active\\ActiveServiceProvider"
      ],
      "aliases": {
        "Active": "Tyjow\\Active\\Facades\\Active"
      }
    }
  }
}
